{
  "name": "cordova.nuxtapp",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "gen": "nuxt generate",
    "generate": "nuxt generate && rsync -r --delete dist/ app/www/ && replace '/nuxt/' 'nuxt/' app/www -r",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint": "npm run lint:js",
    "cordova": "cd app && cordova",
    "run-android": "cd app && cordova build android && cordova run android",
    "run-ios": "cd app && cordova build ios && cordova run ios"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@nuxtjs/axios": "^5.13.6",
    "core-js": "^3.15.1",
    "nuxt": "^2.15.7",
    "replace": "^1.2.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.7",
    "@nuxtjs/eslint-config": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/tailwindcss": "^4.2.0",
    "@types/node": "^18.7.23",
    "eslint": "^7.29.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-vue": "^7.12.1",
    "fibers": "^5.0.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "sass": "^1.34.0",
    "sass-loader": "^10.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
